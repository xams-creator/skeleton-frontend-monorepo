{
    "name": "@skeleton-frontend/react-app",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "@xams-framework/dusk": "latest"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.2",
        "prettier": "latest",
        "ts-loader": "latest",
        "typescript": "latest",
        "xams-cli": "1.3.6-beta4"
    },
    "homepage": "https://xams-creator.github.io/dusk-example-admin",
    "scripts": {
        "start": "xams start",
        "build": "xams build",
        "serve": "cd build && xams serve"
    },

    "eslintConfig": {
        "rules": {
            "jsx-a11y/anchor-is-valid": "off",
            "no-undef": "off",
            "no-restricted-globals": "off",
            "no-unused-vars": "off",
            "no-new-func": "off",
            "import/no-anonymous-default-export": "off",
            "@typescript-eslint/no-unused-vars": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "license": "MIT"
}
